# Token du bot Telegram (à obtenir via BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Clé API pour une blockchain (ex: Infura pour Ethereum)
BLOCKCHAIN_API_KEY=your_blockchain_api_key_here

# Clé de chiffrement pour sécuriser les clés privées des wallets
